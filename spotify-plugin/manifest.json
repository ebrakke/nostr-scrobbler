{
  "manifest_version": 3,
  "name": "Spotify Now Playing",
  "version": "1.0",
  "description": "Display currently playing song on Spotify",
  "permissions": ["identity", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "icons": {
    "128": "images/icon.png"
  }
}